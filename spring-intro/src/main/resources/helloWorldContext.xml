<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!--Config by setter method-->
    <bean id="helloWorld1" class="com.xiaoming.spring.hello_world.HelloWorld">
        <property name="message" value="Spring Hello World!"/>
    </bean>

    <bean id="helloWorld2" class="com.xiaoming.spring.hello_world.HelloWorld">
        <property name="message" value="Spring Hello World 2 !"/>
    </bean>

    <!--Null value-->
    <bean id="helloWorldNull" class="com.xiaoming.spring.hello_world.HelloWorld">
        <property name="message">
            <null/>
        </property>
    </bean>
</beans>